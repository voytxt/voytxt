{
  "name": "voytxt",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "NODE_OPTIONS=--experimental-strip-types vite dev",
    "build": "NODE_OPTIONS=--experimental-strip-types vite build",
    "preview": "NODE_OPTIONS=--experimental-strip-types vite preview",
    "check": "NODE_OPTIONS=--experimental-strip-types svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "NODE_OPTIONS=--experimental-strip-types svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write .",
    "lint": "oxlint"
  },
  "devDependencies": {
    "@sveltejs/adapter-vercel": "^5.8.0",
    "@sveltejs/kit": "^2.25.0",
    "@sveltejs/vite-plugin-svelte": "6.1.0",
    "@vercel/analytics": "^1.5.0",
    "browserslist": "^4.25.1",
    "browserslist-to-esbuild": "^2.1.1",
    "lightningcss": "^1.30.1",
    "oxlint": "^1.7.0",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.4.0",
    "svelte": "^5.36.6",
    "svelte-check": "^4.2.2",
    "typescript": "^5.8.3",
    "vite": "npm:rolldown-vite@^7.0.9",
    "wretch": "^2.11.0"
  },
  "packageManager": "pnpm@10.13.1"
}
